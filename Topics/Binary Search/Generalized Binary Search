ll n,q;
vector<ll> num;

ll bin(ll t){
    if (t < num[0]) return 0;
    ll fir = 0, las = n-1;
    while (fir <= las){
        ll mid = (fir + las)/2;
        if (num[mid] > t){
            las = mid - 1;
        }
        else{
            fir = mid + 1;
        }
    }
    return las + 1;
}

