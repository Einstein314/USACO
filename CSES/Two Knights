    ios::sync_with_stdio(0);
    cin.tie(0);

    cin >> n;
    ll i = 2;
    ll start = 0;
    cout << 0 << "\n";
    while (i <= n){
        ll ans = start + 2 * i * i * i - 3 * i * i - 7 * i + 16;
        cout << ans << "\n";
        start = ans;
        i++;
    }

    return 0;

}








