ll n;
ll arr[1000000];

int main(){
//    ifstream fin("/Users/konwoo/input.txt");
//    ofstream fout("/Users/konwoo/output.txt");
//    ifstream fin("multimoo.in");
//    ofstream fout("multimoo.out");
//    freopen("milkorder.in","r",stdin);
//    freopen("milkorder.out","w",stdout);

    ios::sync_with_stdio(0);
    cin.tie(0);

    cin >> n;
    if (n <= 3 && n >= 2){
        cout << "NO SOLUTION";
    }
    else if (n == 1){
        cout << 1;
    }
    else if (n == 4){
        cout << "3 1 4 2";
    }
    else{
        int i = 4;
        deque<int> d;
        d.push_back(3);
        d.push_back(1);
        d.push_back(4);
        d.push_back(2);
        while (i != n){
            i++;
            if (i % 2 == 1){
                d.push_back(i);
            }
            else{
                d.push_front(i);
            }
        }
        for (int n : d){
            cout << n << " ";
        }
    }

    return 0;

}








