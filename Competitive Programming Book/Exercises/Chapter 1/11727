const int MOD = 1000000000;
double PI = 4*atan(1);


int TC;
int a, b, c;


int main(){
//    ifstream fin("/Users/konwoo/input.txt");
//    ofstream fout("/Users/konwoo/output.txt");
//    ifstream fin("multimoo.in");
//    ofstream fout("multimoo.out");
    scanf("%d", &TC);
    int i = 0;
    while(TC > 0){
        scanf("%d %d %d", &a, &b, &c);
        int m = max(min(a,b), min(max(a,b),c));
        i++;
        printf("Case %d: %d\n", i, m);
        TC--;
    }

    return 0;

}








