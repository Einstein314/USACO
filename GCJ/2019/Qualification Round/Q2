int T, N;
string P, ans;

int main(){
    ios::sync_with_stdio(0);
    cin.tie(0);
    
    cin >> T;
    F0R(i, T){
        cin >> N >> P;
        ans = "";
        F0R(i, 2*N-2){
            if (P[i] == 'E') ans += 'S';
            else ans += 'E';
        }
        cout << "Case #" << i+1 << ": " << ans << "\n";
    }


    

   
    
