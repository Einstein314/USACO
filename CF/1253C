#pragma GCC optimize ("O3")
#pragma GCC target ("sse4")
#include <bits/stdc++.h>
#include <ext/pb_ds/tree_policy.hpp>
#include <ext/pb_ds/assoc_container.hpp>

using namespace std;
using namespace __gnu_pbds;

typedef long long ll;
typedef vector<int> vi;
typedef pair<int, int> pii;

template <class T> using ordered_set = tree<T, null_type, less_equal<T>, rb_tree_tag, tree_order_statistics_node_update>;
typedef tree<int, null_type, less_equal<int>, rb_tree_tag, tree_order_statistics_node_update> ordered_mset;

#define FOR(i,a,b) for(int i = a; i < b; i++)
#define F0R(i,a) for(int i = 0; i < a; i++)
#define pb push_back
#define all(x) x.begin(), x.end()
#define sz(x) int((x).size())
#define newl '\n'

const int MXN = 2e5 + 5;
const ll MOD = 998244353;
const ll INF = 1000000007;



int n, m;
ll a[MXN], pref[MXN], x[MXN];

int main(){
	ios::sync_with_stdio(0);
	cin.tie(0);
	//ifstream cin("/Users/konwoo/input.txt");
	//ofstream cout("/Users/konwoo/output.txt");

	cin >> n >> m;
	F0R(i,n) cin >> a[i+1];
	sort(a+1,a+n+1);
	F0R(i,n) pref[i+1] = pref[i] + a[i+1];
	FOR(i,1,n+1){
		x[i] = pref[i];
		if (i >= m) x[i] += x[i-m];
		cout << x[i] << " ";
	}
	

	return 0;
}

